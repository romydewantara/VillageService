<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvMemberName"
        style="@style/TextBlackRegularRalewayStyle1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama belum terdaftar"
        app:layout_constraintTop_toTopOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvMemberKTP"
        style="@style/TextBlackRegularRalewayStyle1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="00000000xxxxx"
        app:layout_constraintTop_toBottomOf="@id/tvMemberName"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/editButton"
        style="@style/TextBlueBoldRalewayStyle1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edit"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>